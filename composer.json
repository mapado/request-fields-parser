{
    "name": "mapado/request-fields-parser",
    "description": "Convert string like `id,firstname,lastname,jobs{startDate,position,company{id,recordNumber}}` to the array",
    "type": "library",
    "require": {
        "php": "^8.1",
        "doctrine/lexer": "^1.0.1"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Julien Deniau",
            "email": "julien.deniau@mapado.com"
        }
    ],
    "require-dev": {
        "atoum/atoum": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Mapado\\RequestFieldsParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Mapado\\RequestFieldsParser\\Tests\\Units\\": "tests/" }
    }
}
