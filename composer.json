{
  "name": "mapado/request-fields-parser",
  "description": "Convert string like `id,firstname,lastname,jobs{startDate,position,company{id,recordNumber}}` to the array",
  "type": "library",
  "require": {
    "php": "^8.1",
    "doctrine/lexer": "^2.0 || ^3.0"
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Julien Deniau",
      "email": "julien.deniau@mapado.com"
    }
  ],
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^10.3",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Mapado\\RequestFieldsParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mapado\\RequestFieldsParser\\Tests\\Units\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
